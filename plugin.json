{
    "id": "nodebb-plugin-emailer-gmail-oauth2",
    "name": "Emailer (Gmail-Oauth2)",
    "description": "Gmail Emailer for NodeBB",
    "url": "https://github.com/instance-id/nodebb-plugin-emailer-gmail-oauth2",
    "library": "./index.js",
    "templates": "templates",
    "hooks": [
        { "hook": "filter:email.send", "method": "send" },
        { "hook": "filter:admin.header.build", "method": "admin.menu" },
        { "hook": "static:app.load", "method": "init" }
    ]
}